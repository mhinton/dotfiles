let SessionLoad = 1
if &cp | set nocp | endif
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd ~/Sites/uauthor-mupp
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +8 cg_identity/spec/features/user_signs_in_spec.rb
badd +30 cg_identity/spec/spec_helper.rb
badd +9 cg_identity/spec/support/features/session_helpers.rb
badd +0 ~/Sites/uauthor-mupp/cg_identity/spec/support/features.rb
silent! argdel *
set lines=47 columns=183
winpos 0 1028
edit cg_identity/spec/features/user_signs_in_spec.rb
set splitbelow splitright
wincmd _ | wincmd |
split
wincmd _ | wincmd |
split
2wincmd k
wincmd w
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
wincmd w
wincmd t
set winheight=1 winwidth=1
wincmd =
argglobal
let s:l = 16 - ((9 * winheight(0) + 7) / 14)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
16
normal! 0
wincmd w
argglobal
edit cg_identity/spec/support/features/session_helpers.rb
let s:l = 1 - ((0 * winheight(0) + 7) / 15)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
1
normal! 06l
wincmd w
argglobal
edit ~/Sites/uauthor-mupp/cg_identity/spec/support/features.rb
let s:l = 1 - ((0 * winheight(0) + 7) / 15)
